@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@using Umbraco.Web.PublishedContentModels

@{
    Layout = null;
    Html.RequiresCss("https://fonts.googleapis.com/css?family=PT+Sans:400,700|PT+Serif:400,400i,700")
        .RequiresCss("~/content/css/bootstrap.css")
        .RequiresCss("~/content/css/main.css")
        .RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js", 1, 1, new { @defer = "" })
        .RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.1/jquery.min.js", 1, 1, new { @defer = "" })
        .RequiresJs("~/Content/components/picturefill/dist/picturefill.min.js", 1, 2, new { @defer = "" });

    var website = Model.Content.AncestorOrSelf<Website>();
}
<!DOCTYPE html>
