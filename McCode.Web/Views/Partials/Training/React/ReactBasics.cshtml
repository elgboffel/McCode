@inherits UmbracoViewPage

@{
    Html
        .RequiresJs("~/Content/components/react/react.min.js", 1, 3, new { @defer = string.Empty })
        .RequiresJs("~/Content/components/react/react-dom.min.js", 1, 3, new { @defer = string.Empty })
        .RequiresJs("~/Content/components/axios/dist/axios.min.js")
        .RequiresJs("~/scripts/dist/training/react/react.basics.js", 1, 3, new { @defer = string.Empty });
}

<div class="container">
    <div class="row">
        <div class="col-xs-12 col-sm-6">
            <h1>React Basics</h1>
            <div class="react-view"></div>
        </div>
    </div>
</div>